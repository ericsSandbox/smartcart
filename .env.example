# Copy this file to .env and customize values as needed.
# Pricing provider API keys (optional). Leave blank to skip live calls.
FLIPP_API_KEY=
BASKET_API_KEY=

# Enable demo mode for pricing so you can see sample offers without paid keys.
# Accepted truthy values: 1, true, yes, on
PRICING_DEMO=1

# Include Walmart Weekly Deals (often general merchandise). Disabled by default.
# Set to 1/true to include Walmart weekly ad scraping in results.
WEEKLY_WALMART_ENABLED=0
